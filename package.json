{
  "name": "electron-bookshelf",
  "productName": "Electron Bookshelf",
  "version": "1.1.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.dev.config.js",
    "start-dev": "cross-env npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "release": "electron-builder -p always",
    "postinstall": "electron-builder install-app-deps",
    "schema-types": "cross-env DEBUG=1 graphql-codegen"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.{ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "build": {
    "productName": "Electron Bookshelf",
    "appId": "org.your.productname",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "publish": {
      "provider": "github",
      "releaseType": "release"
    }
  },
  "author": {
    "name": "Danut Codrescu",
    "email": "danutzcodrescu@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/ramda": "^0.27.32",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webdriverio": "^5.0.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "image-webpack-loader": "^7.0.1",
    "lint-staged": "^10.5.1",
    "nodemon-webpack-plugin": "^4.3.2",
    "prettier": "^2.1.2",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.1",
    "source-map-loader": "^1.1.2",
    "spectron": "^12.0.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "date-fns": "^2.16.1",
    "electron-updater": "^4.3.5",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "idb": "^5.0.7",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.0",
    "react-query": "^2.26.2",
    "react-query-devtools": "^2.6.3",
    "react-router-dom": "^5.2.0",
    "sqlite3": "^5.0.0",
    "string_decoder": "^1.3.0",
    "typeorm": "^0.2.29"
  }
}
